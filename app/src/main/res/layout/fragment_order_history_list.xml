<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_marginBottom="@dimen/dimen_12"    android:background="@drawable/white_border"    android:elevation="0dp"    android:paddingTop="@dimen/dimen_17"    android:paddingLeft="@dimen/dimen_17"    android:paddingBottom="@dimen/dimen_12"    android:paddingRight="@dimen/dimen_17"    >    <com.hungerbox.customer.util.view.HbTextView        android:id="@+id/tv_order_vendor_name"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignTop="@+id/iv_booking"        android:layout_toRightOf="@+id/iv_booking"        android:text="Vendor Name"        style="@style/NormalTextViewDark"        android:ellipsize="marquee"        android:marqueeRepeatLimit="marquee_forever"        android:scrollHorizontally="true"        android:singleLine="true"        android:maxWidth="220dp"/>    <LinearLayout        android:id="@+id/ll_order_location"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@+id/tv_order_vendor_name"        android:layout_toLeftOf="@+id/tv_order_status"        android:layout_marginTop="@dimen/dimen_2">        <com.hungerbox.customer.util.view.HbTextView            android:id="@+id/tv_order_location"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:gravity="left"            android:layout_below="@+id/tv_order_vendor_name"            android:layout_toLeftOf="@+id/tv_order_status"            android:layout_marginTop="@dimen/dimen_2"            android:text="Order Location"            style="@style/NormalTextViewGrey" />    </LinearLayout>    <com.hungerbox.customer.util.view.HbTextView        android:id="@+id/tv_order_price"        android:layout_width="wrap_content"        android:layout_height="@dimen/dimen_32"        android:layout_marginTop="@dimen/dimen_4"        android:gravity="center"        android:layout_below="@+id/ll_order_location"        style="@style/MediumTextViewBold"        android:text="Rs. 270" />    <ImageView        android:id="@+id/iv_arrow"        android:layout_width="wrap_content"        android:layout_height="@dimen/dimen_32"        android:paddingTop="2dp"        android:paddingBottom="2dp"        android:layout_marginLeft="-10dp"        android:layout_toRightOf="@+id/tv_order_price"        android:layout_alignTop="@+id/tv_order_price"        android:src="@drawable/right_arrow"        />    <ImageView        android:id="@+id/booking_separator"        android:layout_width="match_parent"        android:layout_height="2dp"        android:layout_below="@+id/tv_order_price"        android:layout_marginTop="@dimen/dimen_12"        android:layout_marginRight="?attr/marginSmall"        android:src="@drawable/new_dotted_separator_line"        android:layerType="software"/>    <com.hungerbox.customer.util.view.HbTextView        android:id="@+id/tv_order_products"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/booking_separator"        android:layout_marginTop="@dimen/dimen_10"        style="@style/SmallTextViewGrey"        android:text="Products"        android:padding="0dp"        />    <ImageView        android:id="@+id/tv_order_status"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentRight="true"        android:layout_marginLeft="@dimen/dimen_12"        android:layout_marginRight="@dimen/dimen_20"        android:layout_marginTop="@dimen/dimen_12"        android:visibility="visible"        android:layout_toRightOf="@+id/iv_booking" />    <com.hungerbox.customer.util.view.HbTextView        android:id="@+id/tv_order_date"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/tv_order_products"        android:layout_marginTop="@dimen/dimen_2"        android:layout_toRightOf="@+id/iv_booking"        android:text="21 November 2017"        style="@style/SmallTextViewDark"        />    <Button        android:id="@+id/delivery_date"        android:layout_width="wrap_content"        android:layout_height="20sp"        android:textSize="@dimen/xsmall_text"        android:layout_below="@+id/tv_order_date"        android:layout_marginTop="@dimen/dimen_8"        style="@style/LightButton"        android:text="Delivery Time: 11th Dec, 3:05 PM"        android:clickable="false"        android:textStyle="italic"        android:paddingLeft="12dp"        android:paddingRight="12dp"        android:visibility="gone"        android:fontFamily="@font/groteskreg" />    <Button        android:id="@+id/btn_order_feedback"        android:layout_width="110dp"        android:layout_height="@dimen/dimen_32"        android:layout_below="@+id/delivery_date"        android:layout_marginTop="@dimen/dimen_8"        style="@style/LightButton"        android:text="Feedback"        android:fontFamily="@font/groteskbold"        android:layout_marginBottom="?attr/marginSmall" />    <RatingBar        android:id="@+id/rb_feedback_rating"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        style="?android:attr/ratingBarStyleSmall"        android:layout_marginTop="@dimen/dimen_8"        android:layout_below="@+id/delivery_date"        android:paddingTop="@dimen/dimen_8"        android:paddingBottom="@dimen/dimen_8"        android:isIndicator="true"        android:visibility="gone"        android:stepSize="1" />    <ImageView        android:id="@+id/iv_sentiment_feedback"        android:layout_width="@dimen/dimen_24"        android:layout_height="@dimen/dimen_24"        android:layout_marginTop="?attr/marginSmall"        android:layout_below="@+id/delivery_date"        android:src="@mipmap/ic_sentiment_neutral_black_24dp"        />    <Button        android:id="@+id/btn_reorder"        android:layout_width="wrap_content"        android:layout_height="30dp"        android:layout_alignParentEnd="true"        android:layout_alignParentRight="true"        android:layout_below="@+id/delivery_date"        style="@style/NormalButton"        android:fontFamily="@font/grotesksemibold"        android:layout_marginTop="8dp"        android:text="reorder"        android:textColor="@color/white"        android:visibility="gone"        android:padding="@dimen/padding_small"/></RelativeLayout>